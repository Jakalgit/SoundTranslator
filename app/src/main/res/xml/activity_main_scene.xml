<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/logo"
               motion:framePosition="100"
               android:rotation="360" />
           <KeyPosition />
           <KeyAttribute></KeyAttribute>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="1.0"
            android:layout_height="732dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.666"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="414dp"
            android:id="@+id/backgroundH" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/logo"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.764"
            android:layout_marginTop="100dp" />
        <Constraint
            android:layout_height="55dp"
            android:layout_width="155dp"
            android:id="@+id/signIn"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:layout_marginTop="20dp"
            motion:layout_constraintStart_toStartOf="@+id/password"></Constraint>
        <Constraint
            android:layout_height="55dp"
            android:layout_width="370dp"
            android:id="@+id/password"
            motion:layout_constraintStart_toStartOf="@+id/email1"
            motion:layout_constraintTop_toBottomOf="@+id/email1"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="@+id/email1" ></Constraint>
        <Constraint
            android:layout_height="55dp"
            android:layout_width="155dp"
            android:id="@+id/signUp"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="@+id/password"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="0dp"></Constraint>
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.666"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/background"
            android:layout_marginBottom="0dp"
            motion:layout_editor_absoluteX="15dp" />
        <Constraint
            android:layout_height="31dp"
            android:layout_width="175dp"
            android:id="@+id/error"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            motion:layout_constraintStart_toStartOf="@+id/password"
            motion:layout_constraintEnd_toEndOf="@+id/password"
            android:layout_marginTop="100dp"
            motion:layout_constraintHorizontal_bias="0.502" />
        <Constraint
            android:layout_height="732dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="414dp"
            android:id="@+id/fragment"
            motion:layout_constraintEnd_toEndOf="@+id/background"
            motion:layout_constraintStart_toStartOf="@+id/background"
            motion:layout_constraintTop_toTopOf="@+id/background" />
        <Constraint
            android:layout_height="49dp"
            motion:layout_constraintBottom_toBottomOf="@+id/email1"
            android:layout_width="match_parent"
            android:id="@+id/cardView"
            motion:layout_constraintTop_toTopOf="@+id/background"
            motion:layout_constraintStart_toStartOf="@+id/email1"
            motion:layout_editor_absoluteX="2dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.44"
            motion:layout_constraintEnd_toEndOf="@+id/background"
            android:layout_width="match_parent"
            android:id="@+id/homeLayout"
            motion:layout_editor_absoluteX="147dp" />
        <Constraint
            android:layout_height="55dp"
            android:layout_width="370dp"
            android:id="@+id/welcomeText"
            motion:layout_constraintStart_toStartOf="@+id/email1"
            motion:layout_constraintEnd_toEndOf="@+id/email1"
            motion:layout_constraintBottom_toTopOf="@+id/email1"
            android:layout_marginBottom="20dp" ></Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/background"
            android:id="@+id/cardView2" />
        <Constraint
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_height="55dp"
            android:layout_marginTop="150dp"
            android:layout_width="370dp"
            motion:layout_constraintStart_toStartOf="parent" ></Constraint>
        <Constraint
            android:id="@+id/email1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="150dp" />
        <Constraint
            android:id="@+id/email1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="150dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="130dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="234dp"
            android:id="@+id/logo"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintVertical_bias="0.76"
            motion:layout_constraintHorizontal_bias="0.49"
            android:layout_marginTop="150dp" />
        <Constraint
            motion:layout_constraintVertical_bias="1.0"
            android:layout_height="732dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="414dp"
            android:id="@+id/backgroundH" />
        <Constraint
            android:layout_height="55dp"
            android:layout_width="155dp"
            android:id="@+id/signIn"
            motion:layout_constraintStart_toStartOf="@+id/password"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:layout_marginTop="20dp">
            <CustomAttribute
                motion:attributeName="backgroundColor" />
        </Constraint>
        <Constraint
            android:layout_height="55dp"
            android:layout_width="155dp"
            android:id="@+id/signUp"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:layout_marginTop="20dp"
            motion:layout_constraintEnd_toEndOf="@+id/password">
            <CustomAttribute
                motion:attributeName="backgroundColor" />
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="370dp"
            android:layout_height="55dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="150dp"
            android:id="@+id/email1" >
            <CustomAttribute
                motion:attributeName="backgroundColor" />
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/homeLayout"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_editor_absoluteY="350dp" />
        <Constraint
            android:layout_height="31dp"
            android:layout_width="175dp"
            android:id="@+id/error"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            motion:layout_constraintStart_toStartOf="@+id/password"
            motion:layout_constraintEnd_toEndOf="@+id/password"
            android:layout_marginTop="100dp"
            motion:layout_constraintHorizontal_bias="0.502" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="@+id/email1"
            android:layout_marginTop="20dp"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="@+id/email1"
            android:layout_width="370dp"
            motion:layout_constraintTop_toBottomOf="@+id/email1"
            android:id="@+id/password" >
            <CustomAttribute
                motion:attributeName="backgroundColor" />
        </Constraint>
        <Constraint
            motion:layout_editor_absoluteY="638dp"
            motion:layout_editor_absoluteX="3dp"
            android:id="@+id/cardView" />
        <Constraint
            android:id="@+id/welcomeText"
            motion:layout_constraintEnd_toEndOf="@+id/email1"
            android:layout_width="370dp"
            android:layout_height="55dp"
            android:layout_marginBottom="20dp"
            motion:layout_constraintBottom_toTopOf="@+id/email1"
            motion:layout_constraintStart_toStartOf="@+id/email1" >
            <CustomAttribute
                motion:attributeName="backgroundColor" />
        </Constraint>
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/background" />
    </ConstraintSet>
</MotionScene>